<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata minClientVersion="2.8">
    <id>Cake.CD</id>
    <version>$version$</version>
    <title>Cake Continuous Delivery</title>
    <authors>ObjectivityLtd</authors>
    <projectUrl>https://github.com/ObjectivityLtd/Cake.CD</projectUrl>
    <licenseUrl>https://github.com/ObjectivityLtd/Cake.CD/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Adds Continuous Delivery capabilities to .NET 4 / .NET 5 projects using Cake and dotnet cli.</description>
    <copyright>Objectivity Bespoke Software Specialists, MIT license</copyright>
    <tags>build deploy cake CI CD continuous integration delivery cli</tags>
    <developmentDependency>true</developmentDependency>
    <dependencies>
      <group targetFramework=".NETCoreApp1.0">
        <dependency id="Cake.Common" version="0.19.4" />
        <dependency id="Microsoft.Extensions.CommandLineUtils" version="1.1.0" />
        <dependency id="Microsoft.Extensions.PlatformAbstractions" version="1.1.0" />
      </group>  
    </dependencies>
  </metadata>
  <files>
    <file src="..\src\bin\Release\net*\**" target="lib" exclude="**\templates\**" />
    <file src="..\src\bin\Release\net452\**" target="tools" exclude="**\templates\**" />
    <file src="..\templates\**" target="templates" />
  </files>
</package>